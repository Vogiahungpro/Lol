local BladeBallIDs = {
    13772394625, -- Main
    15234596844, -- Training
    14732610803, -- Pro Server
    16281300371, -- After tutorial
    15144787112, -- Duel lobby (update this after checking)
}

local ForestIDs = {
    79546208627805, -- Original 99 Night in the Forest
    126509999114328 -- New/Alternate place ID
}

local GameID = game.PlaceId

local function isBladeBall(id)
    for _, v in ipairs(BladeBallIDs) do
        if v == id then
            return true
        end
    end
    return false
end

local function isForest(id)
    for _, v in ipairs(ForestIDs) do
        if v == id then
            return true
        end
    end
    return false
end

pcall(function()
    if GameID == 109983668079237 then
        print("Loading Steal a Brainrot script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/Lol"))()
    elseif isBladeBall(GameID) then
        print("Loading Blade Ball script...")
        local success, error = pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/bb"))()
        end)
        if not success then
            warn("Failed to load Blade Ball script: " .. tostring(error))
        end
    elseif GameID == 12979690461 then
        print("Loading Phantom Ball script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/Phantom%20ball"))()
    elseif isForest(GameID) then
        print("Loading 99 Night in the Forest script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/99"))()
    else
        warn("No script configured for this game. Current ID: " .. tostring(GameID))
    end
end)
