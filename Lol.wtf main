local BladeBallIDs = {
    13772394625, -- Main
    15234596844, -- Training
    14732610803, -- Pro Server
    16281300371, -- After tutorial
    15144787112, -- Duel lobby
    14915220621, -- Ranked lobby
    15264892126, -- Ranked match
    17757592456, -- New event sex
}

local GameID = game.PlaceId

local function isBladeBall(id)
    for _, v in ipairs(BladeBallIDs) do
        if v == id then
            return true
        end
    end
    return false
end

pcall(function()
    if isBladeBall(GameID) then
        print("Loading Blade Ball script...")
        local success, error = pcall(function()
            loadstring(game:HttpGet("https://pastefy.app/igrwmRoB/raw"))()
        end)
        if not success then
            warn("Failed to load Blade Ball script: " .. tostring(error))
        end

    elseif GameID == 12979690461 then
        print("Loading Phantom Ball script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/Phantom%20ball"))()
    elseif GameID == 3101667897 then
        print("Loading Legend of speed script...")
        loadstring(game:HttpGet("https://cdn.authguard.org/virtual-file/4f0a458ca08d490085db3ce9f6521a59"))()
    elseif GameID == 18687417158 then
        print("Loading Forsaken script...")
        loadstring(game:HttpGet("https://pastefy.app/4KHoEZVz/raw"))()
    elseif GameID == 537413528 then
        print("Loading Build a boat for treasure script...")
        loadstring(game:HttpGet("https://pastefy.app/0UuJfTkj/raw"))()

    else
        warn("No script configured for this game. Current ID: " .. tostring(GameID))
    end
end)
