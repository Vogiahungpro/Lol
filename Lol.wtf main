local Steal_a_Brainrot = 109983668079237
local BladeBall1 = 13772394625
local BladeBall2 = 15234596844
local PhantomBall = 12979690461
local GameID = game.PlaceId

pcall(function()
    if GameID == Steal_a_Brainrot then
        print("Loading Steal a Brainrot script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/Lol"))()
    elseif GameID == BladeBall1 or GameID == BladeBall2 then
        print("Loading Blade Ball script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/bb"))()
    elseif GameID == PhantomBall then
        print("Loading Phantom Ball script...")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Vogiahungpro/Lol/refs/heads/main/Phantom%20ball"))()
    else
        warn("No script configured for this game.")
    end
end)
